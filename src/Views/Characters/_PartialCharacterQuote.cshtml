@model IEnumerable<TheMatrixAPI.Models.Character.CharacterQuoteDetailsViewModel>

<table class="table table-dark">
    <thead>
        <tr>
            <th>Quote Lines</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var quote in Model)
        {
            <tr>
                <td>@quote.QuoteLine</td>
            </tr>
        }
    </tbody>
</table>
